 CellChangeTimes->{{3.606734717626117*^9, 3.6067347749221773`*^9}}],

Cell["\<\
1a: Consider the function f(x). For what x values is f(x) defined and then \
plot over that interval.\
\>", "Text",
 CellChangeTimes->{{3.606734791936523*^9, 3.606734836687446*^9}}],

Cell[BoxData[
 RowBox[{"Quit", "[", "]"}]], "Input",
 CellChangeTimes->{{3.6067348407055244`*^9, 3.6067348430555773`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"f", "[", "x_", "]"}], "=", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{"x", "^", "2"}], ")"}], "*", 
   RowBox[{"Sqrt", "[", 
    RowBox[{"(", 
     RowBox[{"2", "-", "x", "-", 
      RowBox[{"x", "^", "2"}]}], ")"}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.6067348694171686`*^9, 3.606734906007921*^9}, {
  3.606735846877572*^9, 3.6067358794862413`*^9}}],

Cell[BoxData[
 RowBox[{
  SuperscriptBox["x", "2"], " ", 
  SqrtBox[
   RowBox[{"2", "-", "x", "-", 
    SuperscriptBox["x", "2"]}]]}]], "Output",
 CellChangeTimes->{3.606734911554034*^9, 3.606735880798264*^9, 
  3.606736423445421*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"Sqrt", "[", 
     RowBox[{"(", 
      RowBox[{"2", "-", "x", "-", 
       RowBox[{"x", "^", "2"}]}], ")"}], "]"}], "==", "0"}], ",", "x"}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.606735240635811*^9, 3.6067352931555614`*^9}, {
  3.606736424365441*^9, 3.6067364334116297`*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"x", "\[Rule]", 
     RowBox[{"-", "2"}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", "\[Rule]", "1"}], "}"}]}], "}"}]], "Output",
 CellChangeTimes->{{3.6067352804866247`*^9, 3.606735295185598*^9}, 
   3.6067358859603696`*^9, 3.606736435891679*^9}]
}, Open  ]],

Cell["\<\
Therefore the function is defined on the interval [-2,1]. \
\>", "Text",
 CellChangeTimes->{{3.6067353169240437`*^9, 3.6067353680950975`*^9}}],

